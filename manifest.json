{
  "name": "Emoji replacer",
  "version": "1.0",
  "description": "#relatable",
  "manifest_version": 2,
  "permissions": [
    "activeTab",
    "tabs",
    "http://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "scripts/jquery.js",
        "scripts/contentScript.js"
      ],
      "web_accessible_resources": [
        "res/*"
      ]
    }
  ]
}