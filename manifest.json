{
  "name": "Madlibify",
  "version": "1.0",
  "description": "#relatable",
  "manifest_version": 2,
  "permissions": [
    "activeTab",
    "tabs",
    "http://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "scripts/jquery.js",
        "scripts/materialize.min.js",
        "scripts/contentScript.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "res/*"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Madlibify"
  }
}
